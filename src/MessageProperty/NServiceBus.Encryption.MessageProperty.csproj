<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net452</TargetFramework>
    <AssemblyName>NServiceBus.Encryption.MessageProperty</AssemblyName>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>$(SolutionDir)NServiceBus.snk</AssemblyOriginatorKeyFile>
    <DebugType>full</DebugType>
    <DebugSymbols>True</DebugSymbols>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <OutputPath>..\..\binaries\</OutputPath>
    <NoWarn>CS0419</NoWarn>
    <UpdateAssemblyInfo>true</UpdateAssemblyInfo>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="NServiceBus" Version="6.0.0" />
    <PackageReference Include="Obsolete.Fody" Version="4.2.2" />
    <PackageReference Include="Particular.CodeRules" Version="0.2.0" />
    <PackageReference Include="GitVersionTask" Version="4.0.0-beta0012" />
    <None Include="..\..\GitVersion.yml" Link="GitVersion.yml" />
    <None Include="..\..\packaging\nuget\NServiceBus.Encryption.MessageProperty.nuspec" Link="NServiceBus.Encryption.MessageProperty.nuspec" />
  </ItemGroup>
</Project>